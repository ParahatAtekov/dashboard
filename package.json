{
  "name": "dashboard-backend",
  "version": "1.0.0",
  "description": "Internal SaaS dashboard backend for HyperLiquid-based DeFi platform",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:worker": "ts-node -r tsconfig-paths/register src/workers/worker.ts",
    "dev:scheduler": "ts-node -r tsconfig-paths/register src/workers/scheduler/schedulerRunner.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/worker.js",
    "start:scheduler": "node dist/workers/scheduler/schedulerRunner.js",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "recover-jobs": "ts-node -r tsconfig-paths/register src/scripts/recover-stuck-jobs.ts",
    "monitor-jobs": "ts-node -r tsconfig-paths/register src/scripts/job-monitor.ts"
  },
  "keywords": [
    "hyperliquid",
    "defi",
    "dashboard",
    "analytics"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jose": "^6.1.3",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.8",
    "@types/pg": "^8.16.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}